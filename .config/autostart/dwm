# Restore previous wallpaper
nitrogen --restore 

# Map right Alt key as super
xmodmap ~/.Xmodmap &

# Start Network Manager
NetworkManager &

Thunar --daemon &

# Neovim headless mode (faster startup)
nvim --headless &

# MPD daemon start (if no other user instance exists)
[ ! -s ~/.config/mpd/pid ] && mpd &

dunst &
picom &

xset r rate 200 30 &
xset -b 

exec dwmblocks &
exec dwm
